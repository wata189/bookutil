{
  "name": "bookutil",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "testVueModules": "npx vitest run test.modules.test.ts",
    "testVueComponents": "npx vitest run test.components.test.ts",
    "devVue": "npm run testVueModules && npm run testVueComponents && vite",
    "-------": "",
    "testLambda": "cd lambda-sam && pytest test_lambda.py -v --cov modules_layer",
    "devLambda": "npm run testLambda && cd lambda-sam && sam build && sam local start-api --env-vars env.json --docker-network host",
    "prodLambda": "npm run testLambda && cd lambda-sam && sam build && sam local start-api --env-vars env.production.json --docker-network host",
    "--------": "",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "deployLambda": "npm run testLambda && cd lambda-sam && sam build && sam deploy"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.3",
    "@vue/test-utils": "^2.4.1",
    "amazon-cognito-identity-js": "^6.2.0",
    "axios": "^1.4.0",
    "quasar": "^2.11.10",
    "serverless": "^3.30.1",
    "serverless-python-requirements": "^6.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.3.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^16.18.25",
    "@vitejs/plugin-vue": "^4.1.0",
    "jest": "^29.6.1",
    "jsdom": "^22.1.0",
    "sass": "^1.32.13",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.4.2"
  }
}
